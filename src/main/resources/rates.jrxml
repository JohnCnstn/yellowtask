                    <font size="16" isBold = "true" />
                </textElement>
                <text><![CDATA[Date]]></text>
            </staticText>
        </band>
    </columnHeader>

    <detail>
        <band height="25">

            <textField pattern="dd.MM.yyyy">
                <reportElement x="120" y="0" width="120" height="25"/>
                <textElement>
                    <font size="14" />
                </textElement>
                <textFieldExpression class="java.util.Date">
                    <![CDATA[$F{date}]]></textFieldExpression>
            </textField>

        </band>
    </detail>
</jasperReport>